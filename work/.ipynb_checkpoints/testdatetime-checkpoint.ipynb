    "from datetime import datetime\n",
    "import pytz\n",
    "dt = datetime.now()\n",
    "thai_tz = pytz.timezone('Asia/Bangkok')\n",
    "created_at = dt.replace(tzinfo=thai_tz)\n",
    "\n",
    "local_timestamp = dt.astimezone(thai_tz)\n",
    "\n",
    "print(dt)\n",
    "print(thai_tz)\n",
    "print(created_at)\n",
    "print(local_timestamp)"
   ]
